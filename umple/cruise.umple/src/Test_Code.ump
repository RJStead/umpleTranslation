namespace cruise.umple.compiler;




class UmpleClass {

//return TestCase using name

public UmpleTestCase getUmpleTestCase (String name)
{
	for( UmpleTestCase tc : this.getUmpleTestCases())
	{
		if (tc.getName().equals(name))
		{
			return tc;
		}
	}
	
	return null;

}


}

/*
  Elevator System A - UML class diagram written in Umple
  Last updated: June 8, 2010
*/
generate Java "ElevatorSystemA";

class Building{
 name;
 1 -- 2..* Floor;
 1 -- 1..* Bank;
}

class Bank{
 id;
 1 -- 1..* Elevator;
}


class Floor{
 id;
 doorToOpen;
 
 0..1 -- 0..1 Floor onSamelevelAs;
 1 -- * Elevator onFloor; 
}

class Elevator{
 id;
 direction;
 movingOrStopped;
 * -- * Floor hasAccess;
}


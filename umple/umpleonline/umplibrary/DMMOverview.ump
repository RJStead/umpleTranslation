// This is a UML class diagram written in Umple

// Abstract
class SourceObject{
}

class Defines{
  0..1 -- 1 SourceObject;
  0..1 -- 1 ModelObject;
}

class Declares{
  0..1 -- 1 SourceObject;
  * -- 1 ModelObject;
}

// Abstract
class ModelObject{
  name;
}

// Abstract
class Relationship{}

class SourceRelationship{
  isA Relationship;
}

class ModelElement{
  isA ModelObject;
  }

class ModelRelationship{
  isA ModelObject;
  * toModelRel1 -- 1 ModelElement toModelElem1;
  * toModelRel2 -- 1 ModelElement toModelElem2;
}

class RelationshipInModel{
   isA Relationship;
   1 -- 1 ModelRelationship;
}

class SourceModelRelationship{
  isA Relationship;
}
//$?[End_of_model]$?


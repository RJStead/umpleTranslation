/*
 Example 2: showing how states in traits can be
 obtained by required methods.
*/
trait T1{
  int getData();
  int processData(){
    int data = getData();
    data=data+2;
    setData(data);
    return(data);
  }
}
class C1{
  Integer data;
  isA T1;
}

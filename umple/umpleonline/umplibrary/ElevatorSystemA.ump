//Namespace for core of the system.
namespace Elevator.core;

class Building{
 name;
 1 -- 2..* Floor;
 1 -- 1..* Bank;
}

class Bank{
 id;
 1 -- 1..* Elevator;
}


class Floor{
 id;
 doorToOpen;
 
 0..1 -- 0..1 Floor onSamelevelAs;
 1 -- * Elevator onFloor; 
}

class Elevator{
 id;
 direction;
 movingOrStopped;
 * -- * Floor hasAccess;
}

